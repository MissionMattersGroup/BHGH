@isTest(SeeAllData=true)
public class CandidateTriggerTest {

	@isTest
	static void testBeforeInsertSignatureCreation() {
		Test.startTest();
		Candidate__c testCandidate = (Candidate__c)SmartFactory.createSObject('Candidate__c', true);
		testCandidate.Signature_URL_Long__c = 'data:image/png;base64,';
		insert testCandidate;
		testCandidate = [SELECT Name FROM Candidate__c WHERE Id = :testCandidate.Id];
		System.assert(![SELECT Id FROM Document WHERE Name = :testCandidate.Name].isEmpty());
		testCandidate.Signature_URL_Long__c = 'data:image/png;base64,';
		update testCandidate;
		Test.stopTest();
	}
}
